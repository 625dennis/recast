    "source-map": "0.5.6"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-preset-es2015": "6.24.1",
    "babylon": "6.17.3",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "flow-parser": "0.40.0",
    "mocha": "3.4.2",
    "reify": "0.11.24",
    "semver": "5.3.0"
  },
  "engines": {
    "node": ">= 4.0"
