{
  "author": "Ben Newman <bn@cs.stanford.edu>",
  "name": "@gerhobbelt/recast",
  "description": "JavaScript syntax tree transformer, nondestructive pretty-printer, and automatic source map generator",
  "keywords": [
    "ast",
    "rewriting",
    "refactoring",
    "codegen",
    "syntax",
    "transformation",
    "parsing",
    "pretty-printing"
  ],
  "version": "0.13.0-21",
  "homepage": "http://github.com/benjamn/recast",
  "repository": {
    "type": "git",
    "url": "git://github.com/GerHobbelt/recast.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha --check-leaks --reporter spec --full-trace",
    "debug": "node ./node_modules/mocha/bin/mocha --check-leaks --inspect-brk --reporter spec --full-trace",
    "pub": "npm publish --access public"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "@gerhobbelt/ast-types": "0.10.1-15",
    "@gerhobbelt/esprima": "4.0.1-13",
    "core-js": "2.5.3",
    "private": "0.1.8",
    "source-map": "0.6.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babylon": "7.0.0-beta.31",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "mocha": "4.0.1",
    "flow-parser": "0.61.0",
    "reify": "0.13.3",
    "semver": "5.4.1"
  },
  "engines": {
    "node": ">= 4.0"
  }
}
